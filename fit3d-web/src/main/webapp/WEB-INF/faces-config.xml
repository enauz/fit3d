<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<application>
		<locale-config>
			<default-locale>en_US</default-locale>
		</locale-config>
	</application>
	<converter>
		<converter-id>uuidConverter</converter-id>
		<converter-class>de.bioforscher.fit3d.web.converters.UUIDConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>submitJobView</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.views.SubmitJobView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>jobController</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.JobController</property-class>
			<value>#{jobController}</value>
		</managed-property>
		<managed-property>
			<property-name>sessionController</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.SessionController</property-class>
			<value>#{sessionController}</value>
		</managed-property>
		<managed-property>
			<property-name>jobExecutorService</property-name>
			<property-class>de.bioforscher.fit3d.web.services.JobExecutorService</property-class>
			<value>#{jobExecutorService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jobController</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.controllers.JobController</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>jobExecutorService</property-name>
			<property-class>de.bioforscher.fit3d.web.services.JobExecutorService</property-class>
			<value>#{jobExecutorService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jobListView</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.views.JobListView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>jobJontroller</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.JobController</property-class>
			<value>#{jobController}</value>
		</managed-property>
		<managed-property>
			<property-name>sessionController</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.SessionController</property-class>
			<value>#{sessionController}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resultView</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.views.ResultView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>sessionController</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.SessionController</property-class>
			<value>#{sessionController}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jobExecutorService</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.services.JobExecutorService</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataProvider</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.utilities.DataProvider</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loadMonitorBean</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.utilities.LoadMonitorBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>chartView</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.views.ChartView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>jobJanitorService</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.services.JobJanitorService</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>jobController</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.JobController</property-class>
			<value>#{jobController}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mapBean</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.utilities.MapBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>extractView</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.views.ExtractView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>sessionController</property-name>
			<property-class>de.bioforscher.fit3d.web.controllers.SessionController</property-class>
			<value>#{sessionController}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sessionController</managed-bean-name>
		<managed-bean-class>de.bioforscher.fit3d.web.controllers.SessionController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>submit.xhtml</display-name>
		<from-view-id>/submit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/jobs.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>jobs.xhtml</display-name>
		<from-view-id>/jobs.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/result.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>submit.xhtml</display-name>
		<from-view-id>/submit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/submit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>jobs.xhtml</display-name>
		<from-view-id>/jobs.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/jobs.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<factory>
        <exception-handler-factory>
             de.bioforscher.fit3d.web.exceptions.Fit3DExceptionHandlerFactory
        </exception-handler-factory>
	</factory>
</faces-config>
